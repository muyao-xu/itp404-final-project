<form onsubmit={{action "editRestaurant" model}}>
  <div>
    <label for="name">Name</label>
    {{input type="text" id="name" value=model.name}}
  </div>
  <div>
    <label for="price">price</label>
    {{input type="text" id="price" value=model.price}}
  </div>
  <div>
    <label for="type">Type</label>
    {{input type="text" id="type" value=model.type}}
  </div>

  <button type="submit" data-test="restaurant-edit-submit" >Save</button>
</form>
